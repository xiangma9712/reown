{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/crates/tauri-config-schema/schema.json",
  "productName": "reown",
  "version": "0.1.0",
  "identifier": "com.reown.app",
  "build": {
    "beforeDevCommand": "npm run --prefix frontend dev",
    "beforeBuildCommand": "npm run --prefix frontend build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../frontend/out"
  },
  "app": {
    "windows": [
      {
        "title": "reown",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png"
    ]
  }
}
