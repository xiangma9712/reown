{
  "name": "reown-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,html}' '*.{js,ts,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,html}' '*.{js,ts,json}'",
    "typecheck": "tsc -b --noEmit",
    "test": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:vrt": "npx playwright test --config=playwright-vrt.config.ts",
    "test:vrt:update": "npx playwright test --config=playwright-vrt.config.ts --update-snapshots"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.2.1",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "i18next": "^25.8.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^16.5.4",
    "tailwindcss": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-a11y": "^10.2.12",
    "@storybook/addon-docs": "^10.2.12",
    "@storybook/addon-vitest": "^10.2.12",
    "@storybook/react-vite": "^10.2.12",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-storybook": "^10.2.12",
    "jsdom": "^28.1.0",
    "prettier": "^3.8.1",
    "storybook": "^10.2.12",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.56.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  }
}
